{
    "env": {
        "browser": true,
        "es2021": true
        // "jest": true
    },
    "extends": [
        "plugin:react/recommended",
        "react-app",
        "react-app/jest"
        // "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/jsx-uses-react": ["off"],
        "react/react-in-jsx-scope": ["off"],
        "react/jsx-props-no-spreading": ["off"],
        "react/prop-types": "off",
        "no-shadow": "off",
        "no-unused-vars": ["off"]
        // "react/jsx-props-no-spreading": ["error", {
        //     "custom": "ignore",
        //     "explicitSpread": "ignore"
        //   }]
    }
}
